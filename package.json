{
  "name": "js-express-server",
  "version": "1.0.34",
  "description": "",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "npm run test:lint && npm run test:tsc",
    "test:lint": "eslint --ext \".js,.ts\" ./src",
    "test:tsc": "tsc --noEmit",
